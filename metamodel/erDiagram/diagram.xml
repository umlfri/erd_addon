<?xml version="1.0" encoding="utf-8"?>
<DiagramType id="er_diagram" xmlns="http://umlfri.org/v2/addon.xsd">
    <Icon path="icons/OracleERD.png"/>

    <Structure>
        <Attribute id="name" type="str">
            <Template>
                <Text text="#parent"/>
            </Template>
        </Attribute>
        <Attribute id="note" type="text"/>
    </Structure>

    <DisplayName>
        <Text text="#self.name"/>
    </DisplayName>

    <Elements>
        <Element id="entity"/>
        <Element id="arc"/>
        <Element id="arccircle"/>
    </Elements>

    <Connections>
        <Connection id="relationship"/>
    </Connections>

    <Appearance>
        <Background color="white"/>
    </Appearance>
</DiagramType>
